{
  "name": "tinhte-api",
  "version": "3.3.2",
  "description": "tinhte-api React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "deploy": "npm run build && npm publish && surge --domain https://tinhte-api.surge.sh demo/dist",
    "heroku-postbuild": "next build",
    "heroku-web": "next start -p $PORT",
    "next": "next",
    "start": "nwb serve-react-demo",
    "stats:analyzer": "webpack-bundle-analyzer umd/stats.json umd --host 0.0.0.0 --port 3000",
    "test": "standard && nwb test-react",
    "test:coverage": "standard && nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "isomorphic-unfetch": "^2.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "querystring": "^0.2.0",
    "react-tree-walker": "^4.0.2"
  },
  "peerDependencies": {
    "babel-runtime": "6.x",
    "react": "^16.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "form-data": "^2.3.2",
    "next": "^5.1.0",
    "nwb": "0.21.x",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^4.2.2",
    "sanitize-html": "^1.18.2",
    "standard": "^11.0.0",
    "stats-webpack-plugin": "^0.6.2",
    "surge": "^0.20.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "author": "Dao Hoang Son",
  "homepage": "https://tinhte.vn",
  "license": "MIT",
  "repository": "https://github.com/daohoangson/js-tinhte-api",
  "keywords": [
    "react-component"
  ],
  "standard": {
    "globals": [
      "XenForo",
      "XF",
      "afterEach",
      "beforeEach",
      "describe",
      "it"
    ]
  }
}
